	<queryString language="plsql">
		<![CDATA[SELECT 
COALESCE (e.txt_event_master_name || ''
) AS event_namm,
e.dte_event_date AS event_date, 
COALESCE(
        c.txt_first_name || 
        CASE 
            WHEN c.txt_last_name IS NOT NULL THEN ' ' || c.txt_last_name 
            ELSE '' 
        END,
        '' 
    ) AS client_name,

    COALESCE(c.txt_email, '') AS email,
    COALESCE(c.txt_phone_number_1, '') AS contact_no,

    COALESCE(
        e.txt_bride_first_name ||
        CASE 
            WHEN e.txt_bride_last_name IS NOT NULL THEN ' ' || e.txt_bride_last_name 
            ELSE '' 
        END,
        ''
    ) AS bride_name,

    COALESCE(
        e.txt_groom_first_name ||
        CASE 
            WHEN e.txt_groom_last_name IS NOT NULL THEN ' ' || e.txt_groom_last_name 
            ELSE '' 
        END,
        ''
    ) AS groom_name,
    et.ser_event_type_id,

    COALESCE(v.txt_venue_name, '') AS venue_name,
    COALESCE(et.txt_event_type_name, '') AS event_type_name,
    COALESCE(r.txt_event_running_order_code, '') AS running_order_code

FROM event_master e
LEFT JOIN customer_master c 
       ON e.ser_cust_id = c.ser_cust_id
LEFT JOIN venue_master v 
       ON e.ser_venue_master_id = v.ser_venue_master_id
LEFT JOIN event_type et 
       ON e.ser_event_type_id = et.ser_event_type_id
LEFT JOIN event_running_order r 
       ON e.ser_event_running_order_id = r.ser_event_running_order_id
WHERE e.ser_event_master_id = $P{P_EVENT_MASTER_ID}
  AND e.bln_is_deleted = FALSE]]>
	</queryString>
